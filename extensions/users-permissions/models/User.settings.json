{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": false,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "pushNotificationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "courses": {
      "via": "users",
      "collection": "course",
      "dominant": true
    },
    "point": {
      "type": "integer",
      "default": 0,
      "required": false
    },
    "user_progresses": {
      "via": "users_permissions_user",
      "collection": "user-progress"
    },
    "user_referrers": {
      "via": "referral_referrer",
      "collection": "user-referral"
    },
    "user_referrees": {
      "via": "referral_referree",
      "collection": "user-referral"
    },
    "user_payments": {
      "via": "user",
      "collection": "user-payment"
    },
    "stripeCustomerKey": {
      "type": "string",
      "unique": true
    },
    "Membership": {
      "type": "enumeration",
      "enum": [
        "Gold",
        "Silver",
        "Bronze",
        "Basic"
      ],
      "default": "Basic",
      "required": true,
      "private": false
    },
    "profilePicture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "phone": {
      "type": "string"
    }
  }
}
